@model ExportDemoApp.Models.LaporanViewModel


<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <title>@Model.Title</title>
    </head>
    <body>
        <h1>@Model.Title</h1>
        <table border="1">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Nama</th>
                    <th>Tanggal</th>
                </tr>
            </thead>
            <tbody>
                    @foreach (var item in Model.LaporanList)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Nama</td>
                    <td>@item.Tanggal.ToString("dd/MM/yyyy")</td>
                </tr>
            }
            </tbody>
        </table>
        <p>Tanggal dibuat: @Model.Date.ToString("dd MMMM yyyy")</p>
    </body>
</html>
